{
  "pages": [
    "pages/main/main",
    "pages/index/index",
    "pages/login/login",
    "pages/register/register",
    "pages/map/map",
    "pages/instruction/instruction",
    "pages/location/location",
    "pages/record/record",
    "pages/friends/friends",
    "pages/direction/direction",
    "pages/weather/weather",
    "pages/message/message",
    "pages/BMI/BMI",
    "pages/QR/QR"
  ],
  "window": {
    "backgroundTextStyle": "light",
    "navigationBarBackgroundColor": "#fff",
    "navigationBarTitleText": "我的位置",
    "navigationBarTextStyle": "black"
  },
  "sitemapLocation": "sitemap.json",
  "tabBar": {
    "list": [
      {
        "pagePath": "pages/main/main",
        "iconPath": "./images/note.png",
        "selectedIconPath": "./images/note-selected.png",
        "text": "首页"
      },
      {
        "pagePath": "pages/map/map",
        "iconPath": "./images/map.png",
        "selectedIconPath": "./images/map-selected.png",
        "text": "地图"
      },
      {
        "pagePath": "pages/index/index",
        "iconPath": "./images/mine.png",
        "selectedIconPath": "./images/mine-selected.png",
        "text": "我的"
      }
    ]
  },
  "usingComponents": {
    "mp-form": "weui-miniprogram/form/form",
    "mp-form-page": "weui-miniprogram/form-page/form-page",
    "mp-tabbar": "weui-miniprogram/tabbar/tabbar",
    "mp-cell": "weui-miniprogram/cell/cell",
    "mp-navbar": "weui-miniprogram/navigation-bar/navigation-bar",
    "mp-icon": "weui-miniprogram/icon/icon",
    "mp-search-bar": "weui-miniprogram/searchbar/searchbar",
    "mp-badge": "weui-miniprogram/badge/badge",
    "mp-dialog": "weui-miniprogram/dialog/dialog",
    "van-button": "@vant/weapp/button/index",
    "van-field": "@vant/weapp/field/index",
    "van-cell": "@vant/weapp/cell/index",
    "van-cell-group": "@vant/weapp/cell-group/index",
    "van-radio": "@vant/weapp/radio/index",
    "van-radio-group": "@vant/weapp/radio-group/index",
    "van-checkbox": "@vant/weapp/checkbox/index",
    "van-icon": "@vant/weapp/icon/index",
    "van-image": "@vant/weapp/image/index"
  },
  "useExtendedLib": {
    "kbone": true,
    "weui": true
  },
  "permission": {
    "scope.userLocation": {
      "desc": "你的位置信息将用于小程序位置接口的效果展示"
    }
  },
  "plugins": {
    "chooseLocation": {
      "version": "1.0.5",
      "provider": "wx76a9a06e5b4e693e"
    },
    "routePlan": {
      "version": "1.0.12",
      "provider": "wx50b5593e81dd937a"
    },
    "subway": {
      "version": "1.0.10",
      "provider": "wx6aaf93c4435fa1c1"
    }
  },
  "networkTimeout": {
    "request": 10000,
    "connectSocket": 10000,
    "uploadFile": 10000,
    "downloadFile": 10000
  }
}